{
  "name": "techseovitals-website",
  "version": "1.0.0",
  "description": "TechSEO Vitals",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts",
    "build:dev": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts",
    "start": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --serve",
    "dev": "nodemon --exec \"tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --serve\" --watch src --ext ts,tsx,js,css,md",
    "clean": "rm -rf _site",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:lint": "npm run format && npm run lint:fix",
    "preview": "npm run build && npx vercel dev",
    "deploy": "vercel --prod",
    "deploy:staging": "vercel",
    "optimize:images": "node optimize-images.cjs",
    "perf:bundle": "npm run build && du -sh _site && find _site -name '*.js' -o -name '*.css' | xargs du -sh | sort -hr",
    "audit:deps": "npm audit && npm ls --depth=0"
  },
  "author": "TechSEO Vitals",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "jsx-async-runtime": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^7.1.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^11.0.3",
    "html-minifier-terser": "^7.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.6",
    "terser": "^5.44.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vercel": "^48.1.4"
  },
  "overrides": {
    "esbuild": ">=0.24.3",
    "path-to-regexp": ">=6.3.0",
    "undici": ">=5.28.6",
    "tar": ">=6.2.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
